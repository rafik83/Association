    {% for entity in entities %}
        <tr>
            <td><label class="checkbox no-label" for="checkbox-table-2"><span class="icons"><span class="first-icon fui-checkbox-unchecked"></span><span class="second-icon fui-checkbox-checked"></span></span>
            <span class="icons"><span class="first-icon fui-checkbox-unchecked"></span><span class="second-icon fui-checkbox-checked"></span></span><input type="checkbox" name="entities[]" value="{{ entity.id }}" id="checkbox-table-2" data-toggle="checkbox"></label></td>
            {% for item in app_config.cotisationTable %}
              {% if item.visible %}<td>{{ entity.get(item.fieldName) }}</td>{% endif %}
            {% endfor %}
            <td>
                <a class="btn btn-embossed btn-primary btn-xs" href="{{ path('cotisation_show', { 'id': entity.id }) }}" data-toggle="tooltip" title="{{ 'ben.action.show'|trans }}"><span class="fui-search"></span></a>
                <a class="btn btn-embossed btn-info btn-xs" href="{{ path('cotisation_edit', { 'id': entity.id }) }}" data-toggle="tooltip" title="{{ 'ben.action.edit'|trans }}"><span class="fui-new"></span></a>
            </td>
        </tr>
    {% endfor %}
{% include "BenAssociationBundle:Default:pagination.html.twig" %}

<script>   
(function($) {

    /* tooltip */
    $('td a').tooltip();

})(jQuery);
</script>